<% unless @camps_participated.empty? %>
  <div class="panel clearfix">
    <h5>Camps Participated</h5>
    <ul class="side-nav">
      <% @camps_participated.each do |camp| %>
        <li><%= link_to camp.name, camp_path(camp) %> (<%= camp.start_date.strftime('%m/%d/%y') %>)</li>
      <% end %>
    </ul>
  </div>

<%end%>